{
  "name": "semi-rest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron-copy": "copy \"package.electron.json\" \"dist/semi-rest/package.json\" && copy \"electron.js\" \"dist/semi-rest/electron.js\"",
    "build-electron": "ng build --configuration production --output-hashing none --base-href ./ && npm run electron-copy",
    "electron-win": " build --win --ia32",
    "electron-win-p": " build --win --ia32 -p always",
    "electron-lnx": "build --linux",
    "electron-lnx-p": "build --linux -p always"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@mdi/font": "^7.4.47",
    "angular2-draggable": "^16.0.0",
    "bootstrap": "^4.3.1",
    "ngx-currency": "^17.0.0",
    "primeng": "^17.18.0",
    "rxjs": "^7.8.0",
    "simple-keyboard": "^3.7.0",
    "tslib": "^2.8.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/language-service": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.0.0",
    "electron-builder": "^25.0.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.0"
  }
}
